RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]
RewriteRule ^dashboard/([0-9]+)/([0-9a-zA-Z_-]+) dashboard.php?email=$1&name=$2
Options -Indexes

